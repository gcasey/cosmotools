<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <!-- ================================================================== -->
    <SourceProxy name="vtkPVoronoiReader" class="vtkPVoronoiReader" label="vtkPVoronoiReader">
      <Documentation
         long_help="Read a .out file."
         short_help="Read a .out file.">
      </Documentation>
      <StringVectorProperty
            name="FileName"
            animateable="0"
            command="SetFileName"
            number_of_elements="1">
        <FileListDomain name="files"/>
        <Documentation>
          This property specifies the file name for the PNG reader.
        </Documentation>
      </StringVectorProperty>
 
      <Hints>
        <ReaderFactory extensions="out"
                       file_description="Example File Format" />
     </Hints>
    </SourceProxy>
    <!-- End Reader -->
  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>

